{
    "name": "aedart/athenaeum-config",
    "description": "A loader that is able to load various types of configuration files",
    "keywords": [
        "Athenaeum",
        "Configuration",
        "Loader",
        "ini",
        "Json",
        "PHP Array",
        "yaml"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/config": "^7.6",
        "illuminate/filesystem": "^7.6",
        "aedart/athenaeum-contracts": "^4.0",
        "aedart/athenaeum-support": "^4.0",
        "aedart/athenaeum-utils": "^4.0"
    },
    "require-dev": {
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Config\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
