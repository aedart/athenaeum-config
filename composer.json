{
    "name": "aedart/athenaeum-config",
    "description": "A loader that is able to load various types of configuration files",
    "keywords": ["Athenaeum", "Configuration", "Loader", "ini", "Json", "PHP Array", "yaml", "toml"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-contracts": "^6.2.1",
        "aedart/athenaeum-support": "^6.2.1",
        "aedart/athenaeum-utils": "^6.2.1",
        "illuminate/config": "^v9.7.0",
        "illuminate/filesystem": "^v9.7.0"
    },
    "require-dev": {
        "symfony/yaml": "^v6.0.2",
        "yosymfony/toml": "^1.0.4"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Config\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
